generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model car_owners {
  id    Int    @id @default(autoincrement())
  bio            String?
  car_color      String?
  car_model      String?
  car_model_year String?
  country        String?
  email          String?
  first_name     String?
  gender         String?
}

model users {
  id    Int    @id @default(autoincrement())
  name            String?
  firstname       String?
  email           String?
  lastname        String?
  address         String?
}